    "@nestjs/platform-express": "^9.0.0",
    "@prisma/client": "^4.9.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/hbs": "^4.0.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/passport-facebook": "^2.1.11",
    "@types/passport-google-oauth20": "^2.0.11",
    "cloudinary": "^1.33.0",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.29.3",
    "hbs": "^4.2.0",
    "jsonwebtoken": "^9.0.0",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "prisma": "^4.9.0",
    "quill": "^1.3.7",
